<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mymate_test.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.mymate_test.ui.list.ListFragment"
        android:label="list"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_note"
        android:name="com.example.mymate_test.ui.note.NoteFragment"
        android:label="note"
        tools:layout="@layout/fragment_note" />

    <fragment
        android:id="@+id/navigation_my"
        android:name="com.example.mymate_test.ui.my.MyFragment"
        android:label="my"
        tools:layout="@layout/fragment_my" />
</navigation>